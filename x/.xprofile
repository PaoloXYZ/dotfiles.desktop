# THIS FILE NEEDS TO BE MOVED TO THE HOME DIRECTORY -> ./.xprofile

# set background
feh --bg-fill --randomize ~/images/backgrounds &

# XDG Paths
export XDG_CONFIG_HOME=$HOME/.config
export XDG_CACHE_HOME=$HOME/.cache
export XDG_DATA_HOME=$HOME/.local/share 

# zsh config dir
export ZDOTDIR=$HOME/.config/zsh
